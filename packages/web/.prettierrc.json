{
  "bracketSameLine": true,
  "importOrder": [
    "^@react-components/(.*)$",
    "^@theming/(.*)$",
    "^@tweakwise/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "parser": "typescript",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "printWidth": 120,
  "semi": false,
  "singleQuote": false,
  "useTabs": false,
  "tabWidth": 4,
  "trailingComma": "es5"
}
